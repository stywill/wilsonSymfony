{% extends 'corpo.html.twig' %}
{% block body %}
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>Titulo</th>
                <th>E-mail</th>
                <th>Observações</th>
            </tr>
            {% for c in chamados %}
                <tr>
                    <td>{{ c.getTitulo() }}</td>
                    <td>{{ c.getEmail() }}</td>
                    <td>{{ c.getObservacao() }}</td>
                </tr>
            {% endfor %}

        </table>
    </div>
{% endblock %}

